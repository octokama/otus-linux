OPTIONS_START="-Djava.util.logging.config.file=/root/atlassian-jira-software-7.12.3-standalone/conf/logging.properties -Djava.util.logging.manager=org.apache.juli.ClassLoaderLogManager -Xms384m -Xmx768m -Djava.awt.headless=true -Datlassian.standalone=JIRA -Dorg.apache.jasper.runtime.BodyContentImpl.LIMIT_BUFFER=true -Dmail.mime.decodeparameters=true -Dorg.dom4j.factory=com.atlassian.core.xml.InterningDocumentFactory -XX:-OmitStackTraceInFastThrow -Djira.home=/root/jira -Datlassian.plugins.startup.options= -Djdk.tls.ephemeralDHKeySize=2048 -Djava.protocol.handler.pkgs=org.apache.catalina.webresources -Dorg.apache.catalina.security.SecurityListener.UMASK=0027 -Xloggc:/root/atlassian-jira-software-7.12.3-standalone/logs/atlassian-jira-gc-%t.log -XX:+UseGCLogFileRotation -XX:NumberOfGCLogFiles=5 -XX:GCLogFileSize=20M -XX:+PrintGCDetails -XX:+PrintGCDateStamps -XX:+PrintGCTimeStamps -XX:+PrintGCCause -Dignore.endorsed.dirs= -classpath /root/atlassian-jira-software-7.12.3-standalone/bin/bootstrap.jar:/root/atlassian-jira-software-7.12.3-standalone/bin/tomcat-juli.jar -Dcatalina.base=/root/atlassian-jira-software-7.12.3-standalone -Dcatalina.home=/root/atlassian-jira-software-7.12.3-standalone -Djava.io.tmpdir=/root/atlassian-jira-software-7.12.3-standalone/temp org.apache.catalina.startup.Bootstrap start"
OPTIONS_STOP="-Djava.util.logging.config.file=/root/atlassian-jira-software-7.12.3-standalone/conf/logging.properties -Djava.util.logging.manager=org.apache.juli.ClassLoaderLogManager -Xms384m -Xmx768m -Djava.awt.headless=true -Datlassian.standalone=JIRA -Dorg.apache.jasper.runtime.BodyContentImpl.LIMIT_BUFFER=true -Dmail.mime.decodeparameters=true -Dorg.dom4j.factory=com.atlassian.core.xml.InterningDocumentFactory -XX:-OmitStackTraceInFastThrow -Djira.home=/root/jira -Datlassian.plugins.startup.options= -Djdk.tls.ephemeralDHKeySize=2048 -Djava.protocol.handler.pkgs=org.apache.catalina.webresources -Dorg.apache.catalina.security.SecurityListener.UMASK=0027 -Xloggc:/root/atlassian-jira-software-7.12.3-standalone/logs/atlassian-jira-gc-%t.log -XX:+UseGCLogFileRotation -XX:NumberOfGCLogFiles=5 -XX:GCLogFileSize=20M -XX:+PrintGCDetails -XX:+PrintGCDateStamps -XX:+PrintGCTimeStamps -XX:+PrintGCCause -Dignore.endorsed.dirs= -classpath /root/atlassian-jira-software-7.12.3-standalone/bin/bootstrap.jar:/root/atlassian-jira-software-7.12.3-standalone/bin/tomcat-juli.jar -Dcatalina.base=/root/atlassian-jira-software-7.12.3-standalone -Dcatalina.home=/root/atlassian-jira-software-7.12.3-standalone -Djava.io.tmpdir=/root/atlassian-jira-software-7.12.3-standalone/temp org.apache.catalina.startup.Bootstrap stop"